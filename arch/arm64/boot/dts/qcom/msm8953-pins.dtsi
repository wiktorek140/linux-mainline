	pmx_adv7533_int: pmx_adv7533_int {
		adv7533_int_active: adv7533_int_active {
			pinmux {
				pins = "gpio90";
				function = "gpio";
			};

			pinconf {
				pins = "gpio90";
				drive-strength = <16>;
				bias-disable;
			};
		};

		adv7533_int_suspend: adv7533_int_suspend {
			pinmux {
				pins = "gpio90";
				function = "gpio";
			};

			pinconf {
				pins = "gpio90";
				drive-strength = <16>;
				bias-disable;
			};
		};

	};

	pmx_mdss: pmx_mdss {
		mdss_dsi_active: mdss_dsi_active {
			pinmux {
				pins = "gpio61", "gpio59";
				function = "gpio";
			};

			pinconf {
				pins = "gpio61", "gpio59";
				drive-strength = <8>; /* 8 mA */
				bias-disable = <0>; /* no pull */
				output-high;
			};
		};

		mdss_dsi_suspend: mdss_dsi_suspend {
			pinmux {
				pins = "gpio61", "gpio59";
				function = "gpio";
			};

			pinconf {
				pins = "gpio61", "gpio59";
				drive-strength = <2>; /* 2 mA */
				bias-pull-down; /* pull down */
			};
		};
	};

	mdss_te_default: mdss_te_default {
		pinmux {
			pins = "gpio24";
			function = "mdp_vsync";
		};
		pinconf {
			pins = "gpio24";
			drive-strength = <2>; /* 8 mA */
			bias-pull-down; /* pull down*/
		};
	};

	hsuart_active: default {
		pinmux {
			pins = "gpio12", "gpio13", "gpio14", "gpio15";
			function = "blsp_uart4";
		};

		pinconf {
			pins = "gpio12", "gpio13", "gpio14", "gpio15";
			drive-strength = <16>;
			bias-disable;
		};
	};

	hsuart_sleep: sleep {
		pinmux {
			pins = "gpio12", "gpio13", "gpio14", "gpio15";
			function = "gpio";
		};

		pinconf {
			pins = "gpio12", "gpio13", "gpio14", "gpio15";
			drive-strength = <2>;
			bias-disable;
		};
	};

	blsp2_uart0_active: blsp2_uart0_active {
		pinmux {
			pins = "gpio16", "gpio17", "gpio18", "gpio19";
			function = "blsp_uart5";
		};

		pinconf {
			pins = "gpio16", "gpio17", "gpio18", "gpio19";
			drive-strength = <16>;
			bias-disable;
		};
	};

	blsp2_uart0_sleep: blsp2_uart0_sleep {
		pinmux {
			pins = "gpio16", "gpio17", "gpio18", "gpio19";
			function = "gpio";
		};

		pinconf {
			pins = "gpio16", "gpio17", "gpio18", "gpio19";
			drive-strength = <2>;
			bias-disable;
		};
	};

	/* SDC pin type */
	sdc1_clk_on: sdc1_clk_on {
		pinconf {
			pins = "sdc1_clk";
			bias-disable;		/* NO pull */
			drive-strength = <16>;	/* 16 MA */
		};
	};

	sdc1_clk_off: sdc1_clk_off {
		pinconf {
			pins = "sdc1_clk";
			bias-disable;		/* NO pull */
			drive-strength = <2>;	/* 2 MA */
		};
	};

	sdc1_cmd_on: sdc1_cmd_on {
		pinconf {
			pins = "sdc1_cmd";
			bias-disable;           /* NO pull */
			drive-strength = <10>;	/* 10 MA */
		};
	};

	sdc1_cmd_off: sdc1_cmd_off {
		pinconf {
			pins = "sdc1_cmd";
			num-grp-pins = <1>;
			bias-disable;           /* NO pull */
			drive-strength = <2>;	/* 2 MA */
		};
	};

	sdc1_data_on: sdc1_data_on {
		pinconf {
			pins = "sdc1_data";
			bias-pull-up;		/* pull up */
			drive-strength = <10>;	/* 10 MA */
		};
	};

	sdc1_data_off: sdc1_data_off {
		pinconf {
			pins = "sdc1_data";
			bias-pull-up;		/* pull up */
			drive-strength = <2>;	/* 2 MA */
		};
	};

	sdc1_rclk_on: sdc1_rclk_on {
		pinconf {
			pins = "sdc1_rclk";
			bias-pull-down; /* pull down */
		};
	};

	sdc1_rclk_off: sdc1_rclk_off {
		pinconf {
			pins = "sdc1_rclk";
			bias-pull-down; /* pull down */
		};
	};

	sdc2_clk_on: sdc2_clk_on {
		pinconf {
			pins = "sdc2_clk";
			drive-strength = <16>; /* 16 MA */
			bias-disable; /* NO pull */
		};
	};

	sdc2_clk_off: sdc2_clk_off {
		pinconf {
			pins = "sdc2_clk";
			bias-disable; /* NO pull */
			drive-strength = <2>; /* 2 MA */
		};
	};

	sdc2_cmd_on: sdc2_cmd_on {
