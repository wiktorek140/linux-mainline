			drive-strength = <2>; /* 2 MA */
		};
	};

	sdc2_cd_on: cd_on {
		pinmux {
			pins = "gpio133";
			function = "gpio";
		};

		pinconf {
			pins = "gpio133";
			drive-strength = <2>;
			bias-disable;
		};
	};

	sdc2_cd_off: cd_off {
