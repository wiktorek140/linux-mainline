	spi3 {
		spi3_default: spi3_default {
			/* active state */
			pinmux {
				/* MOSI, MISO, CLK */
				pins = "gpio8", "gpio9", "gpio11";
				function = "blsp_spi3";
			};

			pinconf {
				pins = "gpio8", "gpio9", "gpio11";
				drive-strength = <12>; /* 12 MA */
				bias-disable = <0>; /* No PULL */
			};
		};

		spi3_sleep: spi3_sleep {
			/* suspended state */
			pinmux {
				/* MOSI, MISO, CLK */
				pins = "gpio8", "gpio9", "gpio11";
				function = "gpio";
			};

			pinconf {
				pins = "gpio8", "gpio9", "gpio11";
				drive-strength = <2>; /* 2 MA */
				bias-pull-down; /* PULL Down */
			};
		};

		spi3_cs0_active: cs0_active {
			/* CS */
			pinmux {
				pins = "gpio10";
				function = "blsp_spi3";
			};

			pinconf {
				pins = "gpio10";
				drive-strength = <2>;
				bias-disable = <0>;
			};
		};

		spi3_cs0_sleep: cs0_sleep {
			/* CS */
			pinmux {
				pins = "gpio10";
				function = "gpio";
			};

			pinconf {
				pins = "gpio10";
				drive-strength = <2>;
				bias-disable = <0>;
			};
		};
	};

