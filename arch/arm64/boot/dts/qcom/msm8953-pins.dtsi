			drive-strength = <2>;
			bias-disable;
		};
	};

	i2c_2 {
		i2c_2_default: i2c_2_default {
			/* active state */
			pinmux {
				pins = "gpio6", "gpio7";
				function = "blsp_i2c2";
			};

			pinconf {
				pins = "gpio6", "gpio7";
				drive-strength = <2>;
				bias-disable;
			};
		};

		i2c_2_sleep: i2c_2_sleep {
			/* suspended state */
			pinmux {
				pins = "gpio6", "gpio7";
				function = "gpio";
			};

			pinconf {
				pins = "gpio6", "gpio7";
				drive-strength = <2>;
				bias-disable;
			};
		};
	};

