			/* CLK, DATA */
			pins = "gpio26";
			function = "cam_mclk";
		};

		pinconf {
			pins = "gpio26";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
		};
	};

