#include <dt-bindings/iio/qcom,spmi-vadc.h>
#include <dt-bindings/interrupt-controller/irq.h>
#include <dt-bindings/input/linux-event-codes.h>
#include <dt-bindings/spmi/spmi.h>

&spmi_bus {
	pmi8950_2: pmic@2 {
		compatible = "qcom,pmi8950", "qcom,spmi-pmic";
		reg = <2 SPMI_USID>;
		#address-cells = <1>;
		#size-cells = <0>;

		pon@800 {
			compatible = "qcom,pm8916-pon"; //FIXME is this the correct driver
			reg = <0x800>;			//This pmic may be gen2 unlike 8916
			//qcom,secondary-pon-reset;
			//qcom,hard-reset-poweroff-type = <0x04>;
			pon_pereph-reg {
				//TODO:
				regulator-name = "pon_spare_reg";
			};

			pwrkey {
				compatible = "qcom,pm8941-pwrkey";
				interrupts = <0x0 0x8 0 IRQ_TYPE_EDGE_BOTH>;
				debounce = <15625>;
				bias-pull-up;
				linux,code = <KEY_POWER>;
			};
		};

		rtc@6000 {
			compatible = "qcom,pm8941-rtc";
			reg = <0x6000>;
			reg-names = "rtc", "alarm";
			interrupts = <0x0 0x61 0x1 IRQ_TYPE_NONE>;
		};

		pmi8950_vadc: vadc@3100 {
			compatible = "qcom,spmi-vadc";
			reg = <0x3100>;
			interrupts = <0x2 0x31 0x0 IRQ_TYPE_EDGE_RISING>;
			#address-cells = <1>;
			#size-cells = <0>;
			#io-channel-cells = <1>;

			usbin {
				label = "usbin";
				reg = <VADC_USBIN>;
				qcom,hw-settle-time = <0x0>;
				qcom,pre-scaling = <1 4>;
			};

			dcin {
				label = "dcin";
				reg = <VADC_DCIN>;
				qcom,hw-settle-time = <0x0>;
				qcom,pre-scaling = <1 4>;
			};

			vchg_sns {
				label = "vchg_sns";
				reg = <VADC_VCHG_SNS>;
				qcom,hw-settle-time = <0x0>;
				qcom,pre-scaling = <1 1>;
			};

			ref_625mv {
				label = "ref_625mv";
				reg = <VADC_REF_625MV>;
				qcom,hw-settle-time = <0x0>;
				qcom,pre-scaling = <1 1>;
			};

			ref_1250v {
				label = "ref_1250v";
				reg = <VADC_REF_1250MV>;
				qcom,hw-settle-time = <0x0>;
				qcom,pre-scaling = <1 1>;
			};

			ref_gnd {
				reg = <VADC_GND_REF>;
			};

			ref_vdd {
				reg = <VADC_VDD_VADC>;
			};

			chg_temp {
				reg = <VADC_CHG_TEMP>;
				qcom,hw-settle-time = <0x0>;
			};

			usb_dp {
				reg = <VADC_USB_DP>;
				qcom,hw-settle-time = <0x0>;
			};

			usb_dm {
				reg = <VADC_USB_DM>;
				qcom,hw-settle-time = <0x0>;
			};

			host_det {
				reg = <VADC_P_MUX1_1_1>;
				qcom,hw-settle-time = <0x0>;
			};
		};

		pmi8950_gpios: gpios@c000 {
			compatible = "qcom,pmi8950-gpio";
			reg = <0xc000>;
			gpio-controller;
			#gpio-cells = <2>;
			interrupts = <0 0xc0 0 IRQ_TYPE_NONE>,
				     <0 0xc1 0 IRQ_TYPE_NONE>,
				     <0 0xc2 0 IRQ_TYPE_NONE>,
				     <0 0xc3 0 IRQ_TYPE_NONE>;
		};

		pmi8950_mpps: mpps {
			compatible = "qcom,pm8950-mpp";
			gpio-controller;
			#gpio-cells = <2>;
			#address-cells = <1>;
			#size-cells = <0>;

			mpp@a00 {
				reg = <0xa000>;
				status = "okay";
			};

			mpp@a100 {
				reg = <0xa100>;
				status = "okay";
			};

			mpp@a200 {
				reg = <0xa200>;
				status = "okay";
			};

			mpp@a300 {
				reg = <0xa300>;
				status = "okay";
			};
		};

		qcom_smbcharger {
			compatible = "qcom,pmi8950-charger";
			status = "disabled";
		};

		charger@1000 {
			reg = <0x1000>;
			//FIXME: Add interrupts
		};

		otg@1100 {
			reg = <0x1100>;
			interrupts = <2 0x11 0 IRQ_TYPE_NONE>,
				<2 0x11 1 IRQ_TYPE_NONE>,
				<2 0x11 3 IRQ_TYPE_NONE>;
			interrupts-names = "otg-fail", "otg-oc", "usbid-change";
		};

		bat_if@1200 {
			reg = <0x1200>;
			status = "disabled";
		};

		usb-chgpth@1300 {
			reg = <0x1300>;
			status = "disabled";
		};

		dc-chgpth@1400 {
			reg = <0x1400>;
			status = "disabled";
		};

		chgr-misc@1600 {
			reg = <0x1600>;
			status = "disabled";
		};

		fuel_gauge {
			#address-cells = <1>;
			#size-cells = <0>;
			compatible = "qcom,pmi8950-fg";
			//       soc    bat   memif
			reg = <0x4000 0x4100 0x4400>;
			qcom,cycle-counter-en;
			qcom,capacity-learning-on;
			qcom,batt-warm-temp = <250>;
			qcom,batt-cool-temp = <0>;
			qcom,batt-hot-temp= <600>;
			qcom,batt-cold-temp = <0xffffff38>;

			qcom,recharge-thr = <99>;
			qcom,delta-soc-thr = <1>;
			qcom,cutoff-volt-mv = <3200>;
			qcom,empty-volt-mv = <3000>;
			qcom,fg-vbat-estimate-diff-mv = <1000>;
			qcom,bcl-lm-thr-ma = <126>;
			qcom,bcl-mh-thr-ma = <400>;
			qcom,term-current-ma = <150>;
			qcom,chg-term-current-ma = <100>;
			qcom,fg-cc-cv-thr-mv = <4390>;

			qcom,cl-vbat-est-thr-uv = <300000>;

			qcom,fg-max-soc = <100>;
			qcom,fg-min-soc = <0>;
			qcom,fg-reset-on-lockup;
		};

		bcl@4200 {
			status = "disabled";
		};

		leds@1000 {
			reg = <0x1243>;
			compatible = "qcom,leds-atc";
			label = "charging";
		};
	};

	pmi8950_3: pmi8950@3 {
		compatible = "qcom,pmi8950", "qcom,spmi-pmic";
		reg = <3 SPMI_USID>;
		#address-cells = <1>;
		#size-cells = <0>;

		pwm@b000 {
			status = "disabled";
		};

		qcom-labibb-regulator {
			compatible = "qcom,pmi8998-lab-ibb";
			labibb-mode = "lcd";
			#address-cells = <1>;
			#size-cells = <0>;

			ibb_vreg: ibb@dc00 {
				reg = <0xdc00>;
				regulator-name = "ibb";
				interrupts = <0x03 0xde 0x0 IRQ_TYPE_EDGE_RISING>;
				interrupt-names = "ibb-sc-err";
			};

			lab_vreg: lab@de00 {
				reg = <0xde00>;
				regulator-name = "lab";
				interrupts = <0x03 0xde 0x2 IRQ_TYPE_EDGE_RISING>;
				interrupt-names = "lab-sc-err";
			};
		};

		backlight: wled@d800 {
			#address-cells = <1>;
			#size-cells = <0>;
			status = "disabled"; // FIXME Is this wled stuff even needed?
			compatible = "qcom,pmi8998-wled";
			reg = <0xd800 0xd900>;

			interrupts = <3 0xd8 0x02 IRQ_TYPE_EDGE_RISING>;
			interrupt-names = "short";

			qcom,num-strings = <3>;
			qcom,enabled-strings = <0 1 2 3>;
			/*The fourth string doesn't exist
			Its put there because otherwise the
			driver gets all 0s in its array*/
			qcom,cabc;
			qcom,eternal-pfet;
		};

		flash_leds@d300 {
			compatible = "qcom,flash-leds";
			status = "disabled";
		};

		haptic@c000 {
			compatible = "qcom,haptic";
			status = "disabled";
			reg = <0xc000>;
			interrupts = <3 0xc0 0 IRQ_TYPE_NONE>,
				<3 0xc0 1 IRQ_TYPE_NONE>;
			interrupt-names = "sc-irq", "play-irq";
		};
	};
};
